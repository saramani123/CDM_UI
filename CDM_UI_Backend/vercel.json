{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/main.py",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "PATCH"]
    },
    {
      "src": "/health",
      "dest": "/main.py",
      "methods": ["GET"]
    },
    {
      "src": "/",
      "dest": "/main.py",
      "methods": ["GET"]
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "NEO4J_INSTANCE_NAME": "vulqan-cdm-prod"
  }
}
